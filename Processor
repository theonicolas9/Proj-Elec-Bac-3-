String mess;

PFont f;

void setup(){
  size(600, 180);// canvas size
  background(255);
  f = createFont("Arial",16,true);
  textFont(f,16);
  fill(0);
  println(Serial.list());
  String portName = Serial.list()[0]; // prends le deuxi√®me port com (le port com 11) de la liste de port com qu'on a 
  myPort = new Serial(this, portName, 4800); //port
}

void draw(){
  
  for (int x = 0; x < 10; x++){
    background(255);
    fill(0);
    mess = Trame[x];
    text(mess,10,20);
    myPort.write(mess);
    println(mess);
    delay(1000);
  }

}

